/* moments.wxss */


.date_text {
    font-size: 55rpx;
    color: white;
    transition: opacity 1s ease-out;
}

.weather_text {
    color: rgba(255, 255, 255, 0.8);
    font-size: 28rpx;
    padding-right: 140rpx;
    margin-top: 20rpx;
}

.title {
    font-size: 18px;
    opacity: 0.5;
    padding: 20px 20px 5px 20px;
}

.title_icon {
    max-width: 20px;
    max-height: 20px;
    margin: 0;
    padding: 5px;
    vertical-align: middle;
}



.more_icon {
    max-width: 50px;
    max-height: 50px;
}

.show_more {
    display: inline-block;
    font-size: 14px;
    opacity: 0.2;
    vertical-align: bottom;
    padding: 20px;
    text-align: center;
    align-items: center;
}

.center_text {
    font-size: 14px;
    color: gray;
    text-align: center;
    padding: 10px 0;
}

.ucl-covid-container {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    margin: 0 10px;
} 

.ucl-covid-container .item {
    flex: 1 1 auto;
    text-align: center;
    margin: 0 10px 0 10px;
    padding: 20rpx 40rpx;
    color: grey;
} 

.ucl-covid-container .item .increase  {
    font-size: 25rpx;
}

.ucl-covid-container .item .increase .data {
    color: rgb(215, 16, 30);
}

.ucl-covid-container .item .total {
    color: rgb(215, 16, 30);
    font-size: 1.5em;
}

.ucl-covid-container .item .type  {
    font-size: 28rpx;
}

.block {
    background: white;
    box-shadow: 0px 3px 10px 0px rgba(0, 0, 0, 0.1);
    border-radius: 10px;
    padding: 10px;
    margin: 5px;
}

.block_fullwidth {
    margin: 0 20px;
    padding: 10px 20px;
}

.block_repeated {
    display: inline-block;
    margin-bottom: 10px;
    margin-left: 20px;
    overflow: hidden;
}

.block_timetable {
    height: 150rpx;
    width: 450rpx;
}

.block_post {
    height: 230rpx;
    width: 350rpx;
}

.block_tube {
    height: 150rpx;
    width: 300rpx;
    font-size: 16px;
    text-align: center;
}


.scroll_h {
    width: 100%;
    height: 200rpx;
}

.out {
    width: 100%;
    white-space: nowrap;
    box-sizing: border-box;
}

.tb_secondary {
    font-size: 11px;
    color: silver;
    overflow: scroll;
}

.tb_course {
    font-size: 17px;
    white-space: normal;
}



/* library */
.overview-progress {
    display: flex;
    font-size: 14px;
    line-height: 30px;
    padding: 10px 0 10px 0;
}

.progress-wrapper {
    width: 90%;
    background: #eeeeee;
    display: flex;
    padding-right: 10px;
    border-radius: 30px;
    height: 30px;
    margin: 0 10px 0 0;
    position: relative;

}

.progress-loading {
    background: linear-gradient(135deg,
            rgba(238, 238, 238, 1) 0%,
            rgba(238, 238, 238, 1) 33%,
            rgb(255, 255, 255) 53%,
            rgba(238, 238, 238, 1) 59%,
            rgba(238, 238, 238, 1) 100%);
    background-size: 200% 100%;
    animation: loading 2s ease infinite;
}

.block-loading {
    background: linear-gradient(135deg,
            rgb(248, 248, 248) 0%,
            rgba(248, 248, 248, 1) 33%,
            rgb(255, 255, 255) 53%,
            rgba(248, 248, 248, 1) 59%,
            rgba(248, 248, 248, 1) 100%);
    background-size: 200% 100%;
    animation: loading 2s ease infinite;
}

.progress {
    height: 30px;
    padding-left: 20px;
    background: none;
    border-radius: 30px 0 0 30px;
    width: 0%;
    white-space: nowrap;
    transition: width 2s;
    display: flex;
}

.remain {
    color: gray;
    white-space: nowrap;
}

.indicator {
    padding-right: 5px;
    margin-left: auto;
    color: grey;
    position: absolute;
    right: 8px;
    mix-blend-mode: luminosity;

}

@keyframes loading {
    0% {
        background-position: 120% 0%
    }

    100% {
        background-position: -50% 0%
    }
}



/* posts */
.posts_title {
    /* white-space: normal; */
    font-size: 15px;
    color: black;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.posts_content {
    white-space: normal;
    font-size: 14px;
    color: darkgray;
    overflow: hidden;
    max-height: 80px;
    margin-bottom: auto;
    flex-grow: 1;
}

.posts_time {
    font-size: 12px;
    color: darkgray;
    overflow: hidden;
    margin-top: auto;
    opacity: 0.3;
}

.post_layout {
    display: flex;
    flex-direction: column;
    height: 100%;
}



/* tube */
.tube_line {
    color: white;
    background: grey;
    padding-top: 20px;
    margin: -15px;
    height: 30px;
    margin-bottom: 10px;
    font-size: 14px;
}


.opacity_zero {
    opacity: 0;
}



.admin {
    background: orange;
    color: white;
    text-align: center;
    font-size: 12px;
    position: fixed;
    bottom: 0;
    left: -40px;
    width: 100%;
    z-index: 1000;
}

/* covid */


.mBlock {
    background: white;
    box-shadow: 0px 30px 50px -2px rgba(0, 0, 0, 0.05);
    border-radius: 10px;
    padding: 30px;
    color: #252525;
}

.overview {
    display: flex;
    flex-direction: row;
    justify-content: space-between;

    text-overflow: fade;
    white-space: nowrap;
}

.overview_item {
    margin: 1px 10px;
    text-align: center;
    width: 25% !important;
    font-size: 15px;
}

.overview_title {
    font-size: 12px;
}

.overview_number {
    font-size: 20px;
}

.daily-increase {
    font-size: 14px;
    text-align: center;
}

.covid {
    background: url('https://cloud-minapp-24046.cloud.ifanrusercontent.com/covid-index.png');
    background-size: cover;
    background-position: center center;
    height: 50px;
}

/* base 64 background */

page {
    /* background: url('https://cloud-minapp-24046.cloud.ifanrusercontent.com/index-header.png'), #fafafa; */
    background: url('data:image/png;base64,'), #fafafa;
    background-repeat: no-repeat;
    background-size: contain;
    background-position: top center;
    padding-top: 170rpx;
    animation-name: enter;
    animation-duration: 1s;
}
